<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-42(%d{HH:mm:ss.SSS} [%thread]) %-5level %-54(%logger{54}) - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.crawljax.core.plugin.Plugins" LEVEL="OFF"/>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>